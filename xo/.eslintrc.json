{
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "tabWidth": 2,
          "singleQuote": true,
          "trailingComma": "es5",
          "bracketSpacing": true,
          "jsxBracketSameLine": false
        }
      ]
    }
  }
  